{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "App": {
    "Name": "Saga",
    "Version": 1
  },

  "Cors": {
    "Enabled": true,
    "AllowCredentials": true,
    "AllowedOrigins": [
    ],
    "AllowedMethods": [
      "POST",
      "PUT",
      "DELETE"
    ],
    "AllowedHeaders": [
      "Content-Type",
      "Authorization"
    ],
    "ExposedHeaders": [
      "Resource-ID"
    ]
  }
}
