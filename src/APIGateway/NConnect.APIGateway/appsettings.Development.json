{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
  "ReverseProxy": {
    "Routes": {
      "saga": {
        "ClusterId": "saga",
        "Match": {
          "Path": "saga/{**catchall}"
        },
        "Transforms": [
          {
            "pathPattern": "{**catchall}"
          }
        ]
      }
    },
    "Clusters": {
      "saga": {
        "Destinations": {
          "destination1": {
            "Address": "https+http://saga"
          }
        }
      }
    }
  }
}
